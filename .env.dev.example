# Dev environment skeleton (keys + safe defaults only)
# Copy to .env (DO NOT commit .env)

# ===== Postgres (container) =====
POSTGRES_DB=secp
POSTGRES_USER=
POSTGRES_PASSWORD=

# ===== App DB user (created by Flyway) =====
DB_APP_USER=
DB_APP_PASSWORD=

# ===== Redis (container) =====
SECP_REDIS_HOST=redis
SECP_REDIS_PORT=6379

# ===== Storage mode =====
SECP_STORAGE_MODE=minio

# ===== MinIO (local) =====
MINIO_ROOT_USER=
MINIO_ROOT_PASSWORD=
MINIO_BUCKET=secp-dev

# ===== S3 client config used by API/Worker =====
SECP_S3_ENDPOINT=http://minio:9000
SECP_S3_REGION=us-east-1
SECP_S3_BUCKET=secp-dev
SECP_S3_ACCESS_KEY=
SECP_S3_SECRET_KEY=
SECP_S3_PATH_STYLE=true

# ===== API JWT (dev only) =====
SECP_JWT_SECRET=
